<div class="row">
    <div class="col-md-12">
        <br>
        <h2>Students</h2>
        <hr />

        {{!-- {{#each data}}
        <p>{{firstName}} {{lastName}}</p>
        {{/each}} --}}
        <div class="table-responsive">
            <table class="table">

                <thead>
                    <tr>
                        <th>Student Num</th>
                        <th>Full Name</th>
                        <th>Email</th>
                        <th>Address</th>
                        <th>Status</th>
                        <th>Course ID</th>
                    </tr>
                </thead>

                <tbody>
                    {{#each data}}
                    <tr>
                        <td>{{studentNum}}</td>
                        <td><a href="/student/{{studentNum}}">{{firstName}} {{lastName}}</a></td>
                        <td><a href="mailto:{{email}}">{{email}}</a></td>
                        <td>{{addressStreet}}, {{addressCity}}, {{addressProvince}}</td>
                        <td>{{status}}</td>
                        <td><a href="/course/{{course}}">{{course}}</a></td>
                    </tr>
                    {{/each}}
                </tbody>
        
            </table>
        </div>
    </div>
</div>